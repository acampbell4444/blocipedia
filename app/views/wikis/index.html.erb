<h1>Wikis</h1><br>
  <div style="float:left"><%= link_to "Create a New Wiki", new_wiki_path, class: 'btn btn-success' %></div><br><br><br>
  <% @wikis.each do |wiki| %>
    <% if !(wiki.private) %>
      <p><%= link_to wiki.title, wiki %>
      <small>Created by <%= wiki.user.name || wiki.user.email%></small>
    </p>
    <% end %>
  <% end %>
