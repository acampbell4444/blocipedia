<br><div style="float:left">
  <h1>Wikis</h1>
  <%= link_to "Create a New Wiki", new_wiki_path, class: 'btn btn-success' %><br><br>

  <% @wikis.each do |wiki| %>
    <% if !(wiki.private) %>
      <p><%= link_to wiki.title, wiki %></p>
    <% end %>
  <% end %>

</div>

<div style="float:right">
  <%= render partial: 'private_wikis', locals: {wiki: @wiki} %>
</div>
